# SpendSmart Development Environment
NODE_ENV=development
LOG_LEVEL=debug

# Database (update with your local database details)
DATABASE_URL=postgresql://postgres:@localhost:5432/spendsmartdev

# Authentication (use a secure random string in production)
JWT_SECRET=dev-jwt-secret-change-in-production-must-be-at-least-32-chars

# Backend Configuration
PORT=3001
CORS_ORIGIN=http://localhost:3000

# Frontend Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_APP_NAME=SpendSmart
VITE_ENABLE_ANALYTICS=false

# Development Features
ENABLE_SWAGGER=true
ENABLE_DEBUG_LOGS=true
MOCK_EXTERNAL_APIS=true
TINK_ENVIRONMENT=sandbox 

# Google OAuth (get these from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here